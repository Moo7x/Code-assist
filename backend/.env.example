# x402 Facilitator URL
NEXT_PUBLIC_FACILITATOR_URL=https://x402.org/facilitator

# CAIP-2 network identifier (Base Sepolia testnet)
NETWORK=eip155:84532

# Treasury wallet address - replace with your wallet address
RESOURCE_WALLET_ADDRESS=0xYourWalletAddressHere

# GitHub OAuth configuration
# 1. Visit https://github.com/settings/developers
# 2. Create OAuth App, Callback URL: http://localhost:3000/api/auth/github/callback
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
NEXT_PUBLIC_APP_URL=http://localhost:3000
